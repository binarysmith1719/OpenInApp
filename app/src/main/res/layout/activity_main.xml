<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <ScrollView
        android:layout_width="match_parent"
        android:background="#818589"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <LinearLayout
            android:layout_height="match_parent"
            android:id="@+id/llLANG"
            android:background="@color/teal_200"
            android:orientation="vertical"
            android:layout_width="match_parent">
           <RelativeLayout
               android:layout_width="match_parent"
               android:background="#B2BEB5"
               android:layout_height="match_parent">
              <TextView
                  android:layout_width="wrap_content"
                  android:id="@+id/greeting"
                  android:text="Good Morning"
                  android:textStyle="bold"
                  android:textColor="@color/white"
                  android:fontFamily="@font/quicksand_regular"
                  android:textSize="20sp"
                  android:layout_marginTop="10dp"
                  android:layout_alignParentTop="true"
                  android:layout_marginHorizontal="20dp"
                  android:layout_height="wrap_content"/>
               <TextView
                   android:layout_width="wrap_content"
                   android:id="@+id/idname"
                   android:text="SUBHAM KUMAR"
                   android:fontFamily="@font/quicksand_bold"
                   android:textStyle="bold"
                   android:textColor="@color/white"
                   android:textSize="30sp"
                   android:layout_below="@+id/greeting"
                   android:layout_marginBottom="10dp"
                   android:layout_marginHorizontal="20dp"
                   android:layout_height="wrap_content"/>
              <androidx.cardview.widget.CardView
                  android:layout_width="wrap_content"
                  android:id="@+id/cardGraph"
                  android:layout_below="@+id/idname"
                  android:layout_marginTop="5dp"
                  android:layout_marginHorizontal="10dp"
                  app:cardCornerRadius="10dp"
                  android:layout_height="wrap_content">
                  <RelativeLayout
                      android:layout_width="wrap_content"
                      android:layout_marginVertical="20dp"
                      android:layout_height="wrap_content">
               <com.jjoe64.graphview.GraphView
                   android:id="@+id/graph"
                   android:layout_width="match_parent"
                   android:layout_height="250dp"
                   android:background="@color/white"
                   android:layout_alignParentTop="true"
                   ></com.jjoe64.graphview.GraphView>
                  </RelativeLayout>
              </androidx.cardview.widget.CardView>
               <HorizontalScrollView
                   android:layout_width="match_parent"
                   android:scrollbars="none"
                   android:background="#B2BEB5"
                   android:layout_below="@+id/cardGraph"
                   android:padding="5dp"
                   android:layout_height="wrap_content">
                   <LinearLayout
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content">
                       <androidx.cardview.widget.CardView
                           android:layout_marginVertical="5dp"
                           android:layout_marginLeft="5dp"
                           app:cardCornerRadius="10dp"
                           android:id="@+id/card2"
                           android:layout_marginRight="5dp"
                           android:layout_width="120dp"
                           android:layout_height="120dp">
                          <RelativeLayout
                              android:layout_width="match_parent"
                              android:layout_height="match_parent">
                              <ImageView
                                  android:layout_width="40dp"
                                  android:layout_margin="8dp"
                                  android:layout_alignParentTop="true"
                                  android:src="@drawable/todaysclick"
                                  android:layout_height="40dp">
                              </ImageView>
                              <TextView
                                  android:layout_width="wrap_content"
                                  android:layout_marginBottom="10dp"
                                  android:layout_marginLeft="8dp"
                                  android:text="123"
                                  android:fontFamily="@font/quicksand_bold"

                                  android:textStyle="bold"
                                  android:id="@+id/todaysClick"
                                  android:layout_above="@+id/clicks"
                                  android:layout_height="wrap_content">

                              </TextView>
                              <TextView
                                  android:layout_width="wrap_content"
                                  android:text="Today's Clicks"
                                  android:id="@+id/clicks"
                                  android:layout_alignParentBottom="true"
                                  android:layout_marginBottom="10dp"
                                  android:layout_marginLeft="8dp"
                                  android:textStyle="normal"
                                  android:layout_height="wrap_content"/>
                          </RelativeLayout>
                       </androidx.cardview.widget.CardView>
                       <androidx.cardview.widget.CardView
                           android:layout_marginVertical="5dp"
                           android:layout_marginLeft="5dp"
                           app:cardCornerRadius="10dp"
                           android:id="@+id/card8"
                           android:layout_marginRight="5dp"
                           android:layout_width="120dp"
                           android:layout_height="120dp">
                           <RelativeLayout
                               android:layout_width="match_parent"
                               android:layout_height="match_parent">

                               <ImageView
                                   android:layout_width="40dp"
                                   android:layout_height="40dp"
                                   android:layout_alignParentTop="true"
                                   android:layout_marginStart="8dp"
                                   android:layout_marginTop="8dp"
                                   android:layout_marginEnd="8dp"
                                   android:layout_marginBottom="8dp"
                                   android:src="@drawable/totalclicks"></ImageView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:text="123"
                                   android:textStyle="bold"
                                   android:fontFamily="@font/quicksand_bold"

                                   android:id="@+id/totalClicks"
                                   android:layout_above="@+id/totClicks"
                                   android:layout_height="wrap_content">

                               </TextView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:text="Total Clicks"
                                   android:id="@+id/totClicks"
                                   android:layout_alignParentBottom="true"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:textStyle="normal"
                                   android:layout_height="wrap_content"/>
                           </RelativeLayout>
                       </androidx.cardview.widget.CardView>
                       <androidx.cardview.widget.CardView
                           android:layout_marginVertical="5dp"
                           android:layout_marginLeft="5dp"
                           app:cardCornerRadius="10dp"
                           android:id="@+id/card1"
                           android:layout_marginRight="5dp"
                           android:layout_width="120dp"
                           android:layout_height="120dp">
                           <RelativeLayout
                               android:layout_width="match_parent"
                               android:layout_height="match_parent">
                               <ImageView
                                   android:layout_width="40dp"
                                   android:layout_margin="8dp"
                                   android:layout_alignParentTop="true"
                                   android:src="@drawable/location"
                                   android:layout_height="40dp">
                               </ImageView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:fontFamily="@font/quicksand_bold"
                                   android:singleLine="true"
                                   android:text="123"
                                   android:textStyle="bold"
                                   android:id="@+id/location"
                                   android:layout_above="@+id/topLocation"
                                   android:layout_height="wrap_content">

                               </TextView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:text="Top Location"
                                   android:id="@+id/topLocation"
                                   android:layout_alignParentBottom="true"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:textStyle="normal"
                                   android:layout_height="wrap_content"/>
                           </RelativeLayout>
                       </androidx.cardview.widget.CardView>
                       <androidx.cardview.widget.CardView
                           android:layout_marginVertical="5dp"
                           android:layout_marginLeft="5dp"
                           app:cardCornerRadius="10dp"
                           android:id="@+id/card3"
                           android:layout_marginRight="5dp"
                           android:layout_width="120dp"
                           android:layout_height="120dp">
                           <RelativeLayout
                               android:layout_width="match_parent"
                               android:layout_height="match_parent">
                               <ImageView
                                   android:layout_width="40dp"
                                   android:layout_margin="8dp"
                                   android:layout_alignParentTop="true"
                                   android:src="@drawable/globe"
                                   android:layout_height="40dp">
                               </ImageView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:text="123"
                                   android:textStyle="bold"
                                   android:fontFamily="@font/quicksand_bold"

                                   android:id="@+id/source"
                                   android:layout_above="@+id/top_source"
                                   android:layout_height="wrap_content">

                               </TextView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:text="Top Source"
                                   android:id="@+id/top_source"
                                   android:layout_alignParentBottom="true"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:textStyle="normal"
                                   android:layout_height="wrap_content"/>
                           </RelativeLayout>
                       </androidx.cardview.widget.CardView>
                       <androidx.cardview.widget.CardView
                           android:layout_marginVertical="5dp"
                           android:layout_marginLeft="5dp"
                           app:cardCornerRadius="10dp"
                           android:id="@+id/card4"
                           android:layout_marginRight="5dp"
                           android:layout_width="120dp"
                           android:layout_height="120dp">
                           <RelativeLayout
                               android:layout_width="match_parent"
                               android:layout_height="match_parent">
                               <ImageView
                                   android:layout_width="40dp"
                                   android:layout_height="40dp"
                                   android:layout_alignParentTop="true"
                                   android:layout_marginStart="8dp"
                                   android:layout_marginTop="8dp"
                                   android:layout_marginEnd="8dp"
                                   android:layout_marginBottom="8dp"
                                   android:src="@drawable/income"></ImageView>

                               <TextView
                                   android:layout_width="wrap_content"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:text="123"
                                   android:fontFamily="@font/quicksand_bold"

                                   android:textStyle="bold"
                                   android:id="@+id/income"
                                   android:layout_above="@+id/extra_income"
                                   android:layout_height="wrap_content">

                               </TextView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:text="Extra Income"
                                   android:id="@+id/extra_income"
                                   android:layout_alignParentBottom="true"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:textStyle="normal"
                                   android:layout_height="wrap_content"/>
                           </RelativeLayout>
                       </androidx.cardview.widget.CardView>
                       <androidx.cardview.widget.CardView
                           android:layout_marginVertical="5dp"
                           android:layout_marginLeft="5dp"
                           app:cardCornerRadius="10dp"
                           android:id="@+id/card5"
                           android:layout_marginRight="5dp"
                           android:layout_width="120dp"
                           android:layout_height="120dp">
                           <RelativeLayout
                               android:layout_width="match_parent"
                               android:layout_height="match_parent">
                               <ImageView
                                   android:layout_width="40dp"
                                   android:layout_height="40dp"
                                   android:layout_alignParentTop="true"
                                   android:layout_marginStart="8dp"
                                   android:layout_marginTop="8dp"
                                   android:layout_marginEnd="8dp"
                                   android:layout_marginBottom="8dp"
                                   android:src="@drawable/link"></ImageView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:text="123"
                                   android:textStyle="bold"
                                   android:fontFamily="@font/quicksand_bold"

                                   android:id="@+id/links"
                                   android:layout_above="@+id/top_links"
                                   android:layout_height="wrap_content">

                               </TextView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:text="Top Links"
                                   android:id="@+id/top_links"
                                   android:layout_alignParentBottom="true"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:textStyle="normal"
                                   android:layout_height="wrap_content"/>
                           </RelativeLayout>
                       </androidx.cardview.widget.CardView>
                       <androidx.cardview.widget.CardView
                           android:layout_marginVertical="5dp"
                           android:layout_marginLeft="5dp"
                           app:cardCornerRadius="10dp"
                           android:id="@+id/card6"
                           android:layout_marginRight="5dp"
                           android:layout_width="120dp"
                           android:layout_height="120dp">
                           <RelativeLayout
                               android:layout_width="match_parent"
                               android:layout_height="match_parent">
                               <ImageView
                                   android:layout_width="40dp"
                                   android:layout_height="40dp"
                                   android:layout_alignParentTop="true"
                                   android:layout_marginStart="8dp"
                                   android:layout_marginTop="8dp"
                                   android:layout_marginEnd="8dp"
                                   android:layout_marginBottom="8dp"
                                   android:src="@drawable/campaign"></ImageView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:text="123"
                                   android:textStyle="bold"
                                   android:fontFamily="@font/quicksand_bold"

                                   android:id="@+id/campaign"
                                   android:layout_above="@+id/applied_campaign"
                                   android:layout_height="wrap_content">

                               </TextView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:text="Applied Campaign"
                                   android:id="@+id/applied_campaign"
                                   android:layout_alignParentBottom="true"
                                   android:singleLine="true"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:textStyle="normal"
                                   android:layout_height="wrap_content"/>
                           </RelativeLayout>
                       </androidx.cardview.widget.CardView>
                       <androidx.cardview.widget.CardView
                           android:layout_marginVertical="5dp"
                           android:layout_marginLeft="5dp"
                           app:cardCornerRadius="10dp"
                           android:id="@+id/card7"
                           android:layout_marginRight="5dp"
                           android:layout_width="120dp"
                           android:layout_height="120dp">
                           <RelativeLayout
                               android:layout_width="match_parent"
                               android:layout_height="match_parent">
                               <ImageView
                                   android:layout_width="40dp"
                                   android:layout_height="40dp"
                                   android:layout_alignParentTop="true"
                                   android:layout_marginStart="8dp"
                                   android:layout_marginTop="8dp"
                                   android:layout_marginEnd="8dp"
                                   android:layout_marginBottom="8dp"
                                   android:src="@drawable/time"></ImageView>

                               <TextView
                                   android:layout_width="wrap_content"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:text="123"
                                   android:textStyle="bold"
                                   android:id="@+id/time"
                                   android:fontFamily="@font/quicksand_bold"
                                   android:layout_above="@+id/startTime"
                                   android:layout_height="wrap_content">

                               </TextView>
                               <TextView
                                   android:layout_width="wrap_content"
                                   android:text="Start Time"
                                   android:id="@+id/startTime"
                                   android:layout_alignParentBottom="true"
                                   android:layout_marginBottom="10dp"
                                   android:layout_marginLeft="8dp"
                                   android:textStyle="normal"
                                   android:layout_height="wrap_content"/>
                           </RelativeLayout>
                       </androidx.cardview.widget.CardView>
                   </LinearLayout>
               </HorizontalScrollView>
           </RelativeLayout>
            <com.google.android.material.tabs.TabLayout
                android:id="@+id/tabl"
                android:layout_height="?attr/actionBarSize"
                android:layout_width="match_parent">
            </com.google.android.material.tabs.TabLayout>
            <androidx.viewpager.widget.ViewPager
                android:id="@+id/viewpagerl"
                android:background="#B2BEB5"
                android:layout_width="match_parent"
                android:layout_height="700dp"
                />
        </LinearLayout>
        </RelativeLayout>
    </ScrollView>

    <ProgressBar
        android:layout_width="wrap_content"
        android:id="@+id/progbar"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content">
    </ProgressBar>
</RelativeLayout>